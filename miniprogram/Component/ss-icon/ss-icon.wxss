@font-face {
    font-family: "ss-font";
    src: url('iconfont.eot?t=1559192589401');
    /* IE9 */
    src: url('iconfont.eot?t=1559192589401#iefix') format('embedded-opentype'),
        /* IE6-IE8 */
        url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
        url('iconfont.woff?t=1559192589401') format('woff'),
        url('iconfont.ttf?t=1559192589401') format('truetype'),
        /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
        url('iconfont.svg?t=1559192589401#ss-font') format('svg');
    /* iOS 4.1- */
}

.ss-font {
    font-family: "ss-font" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.ss-iconback:before {
    content: "\e609";
}

.ss-iconhome:before {
    content: "\e615";
}

.ss-icon__info {
    color: #fff;
    left: 100%;
    top: -.5em;
    font-size: 0.5em;
    padding: 0 0.3em;
    text-align: center;
    min-width: 1.2em;
    line-height: 1.2;
    position: absolute;
    border-radius: 0.6em;
    box-sizing: border-box;
    background-color: #f44;
    -webkit-transform: translateX(-50%);
    transform: translateX(-50%);
    font-family: PingFang SC, Helvetica Neue, Arial, sans-serif;
}